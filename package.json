{
    "name": "spacex-frontend-app",
    "version": "1.0.0",
    "description": "Server Side Redered React Application with Node Express Framework",
    "main": "index.js",
    "engines": {
        "node": ">=10.15"
    },
    "scripts": {
        "test": "node script/test.js",
        "build": "npm run format && gulp && cross-env NODE_ENV=production node_modules/.bin/webpack --config=webpack.production.js --mode=production -p",
        "start": "cross-env NODE_ENV=development node_modules/.bin/babel-node server.js",
        "dev": "cross-env NODE_ENV=dev node_modules/.bin/babel-node server.js",
        "lint": "eslint src/**/*.js",
        "format": "prettier --write '**/*.{js,json,css,scss,md}'"
    },
    "lint-staged": {
        "*.js": [
            "npm run lint"
        ],
        "*.{js,json,css,md}": [
            "npm run format",
            "git add"
        ]
    },
    "transform": {
        "\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "dependencies": {
        "@babel/cli": "^7.4.4",
        "@babel/core": "^7.6.2",
